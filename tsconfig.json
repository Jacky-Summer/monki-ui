{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ESNext",
    "moduleResolution": "Node",
    "importHelpers": true,
    "strictNullChecks": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@wojtekmaj/enzyme-adapter-react-17": ["./types/enzyme-adapter-react-17"]
    }
  },
  "exclude": ["node_modules", "dist", "typings"]
}
