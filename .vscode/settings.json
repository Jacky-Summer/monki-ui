{
  "search.exclude": {
    "**/node_modules": true,
    "dist": true,
    "docs-dist": true,
    "yarn.lock": true,
    "src/.umi": true
  },
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "css.validate": false,
  "less.validate": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  }
}
